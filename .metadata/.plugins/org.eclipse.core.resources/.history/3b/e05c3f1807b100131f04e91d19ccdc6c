class Point {
	
	private double x;
	private double y;
	
	Point(double x, double y) {
		setX(x);
		setY(y);
	}
	
	public double getX() {
		return x;
	}

	public void setX(double x) {
		this.x = x;
	}

	public double getY() {
		return y;
	}

	public void setY(double y) {
		this.y = y;
	}	
	
	public double distance(Point p) {
		return Math.pow(getX()-p.getX(), getY()-p.getY());
	}
	
}

